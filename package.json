{
  "name": "kqs.fe.ssr",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start"
  },
  "dependencies": {
    "@heroicons/react": "^1.0.1",
    "include-media": "^1.4.10",
    "include-media-export": "^1.0.3",
    "next": "10.2.0",
    "normalize.css": "^8.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@emotion/css": "^11.1.3",
    "@emotion/react": "^11.4.0",
    "@types/node": "^15.0.3",
    "@types/react": "^17.0.5",
    "@typescript-eslint/parser": "^4.23.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "node-sass": "^5.0.0",
    "open-color": "^1.8.0",
    "prettier": "^2.3.0",
    "typescript": "^4.2.4"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "airbnb",
      "airbnb/hooks",
      "plugin:prettier/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {
      "prettier/prettier": "off",
      "import/extensions": "off",
      "no-console": "off",
      "max-len": [
        "error",
        120
      ],
      "no-undef": "off",
      "react/react-in-jsx-scope": "off",
      "react/jsx-filename-extension": "off",
      "react/jsx-props-no-spreading": "off"
    },
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [
            ".js",
            ".jsx",
            ".ts",
            ".tsx",
            "d.ts"
          ]
        }
      }
    }
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 120
  }
}
